package com.epam.stud.classloader;

public class Runner {
	public static void main(String[] args) {
		TestModule t = new TestModule();
		while (true) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println(t);
		}
	}
}
